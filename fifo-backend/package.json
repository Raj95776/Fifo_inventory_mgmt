    "export:ml": "ts-node src/jobs/export-transactions.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.10.0",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.14",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "fast-csv": "^5.0.5",
    "kill-port": "^2.0.1",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.1",
    "prisma": "^5.10.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  }
